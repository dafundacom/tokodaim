{
  "name": "@tokodaim/locales",
  "version": "0.0.2",
  "private": true,
  "description": "",
  "keywords": [
    "tokodaim",
    "locales"
  ],
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "src"
  ],
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": {
        "types": "./src/index.ts",
        "default": "./src/index.ts"
      },
      "require": {
        "types": "./src/index.ts",
        "default": "./src/index.ts"
      }
    },
    "./client": {
      "source": "./src/client.ts",
      "import": {
        "types": "./src/client.ts",
        "default": "./src/client.ts"
      },
      "require": {
        "types": "./src/client.ts",
        "default": "./src/client.ts"
      }
    },
    "./server": {
      "source": "./src/server.ts",
      "import": {
        "types": "./src/server.ts",
        "default": "./src/server.ts"
      },
      "require": {
        "types": "./src/server.ts",
        "default": "./src/server.ts"
      }
    },
    "./middleware": {
      "source": "./src/middleware.ts",
      "import": {
        "types": "./src/middleware.ts",
        "default": "./src/middleware.ts"
      },
      "require": {
        "types": "./src/middleware.ts",
        "default": "./src/middleware.ts"
      }
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "author": "Karyana Yandi <karyana@yandi.me>",
  "license": "AGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/dafundacom/tokodaim",
    "directory": "packages/locales"
  },
  "bugs": {
    "url": "https://github.com/dafundacom/tokodaim/issues"
  },
  "scripts": {
    "format:write": "prettier --write \"**/*.{ts,tsx,mts,js,jsx,mjs,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,mts,js,jsx,mjs,json,md}\"",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@yopem/eslint-config": "catalog:tooling",
    "@yopem/prettier-config": "catalog:tooling",
    "@yopem/typescript-config": "catalog:tooling",
    "eslint": "catalog:tooling",
    "prettier": "catalog:tooling",
    "typescript": "catalog:tooling"
  },
  "dependencies": {
    "next-international": "^1.3.1"
  },
  "prettier": "@yopem/prettier-config/base"
}
