    "@tiptap/extension-character-count": "2.9.1",
    "@tiptap/extension-code-block": "^2.9.1",
    "@tiptap/extension-code-block-lowlight": "2.9.1",
    "@tiptap/extension-color": "2.9.1",
    "@tiptap/extension-document": "2.9.1",
