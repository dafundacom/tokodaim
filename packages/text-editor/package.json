{
  "name": "@tokodaim/text-editor",
  "version": "0.0.2",
  "private": true,
  "description": "",
  "keywords": [
    "tokodaim",
    "text-editor"
  ],
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "src"
  ],
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": {
        "types": "./src/index.ts",
        "default": "./src/index.ts"
      },
      "require": {
        "types": "./src/index.ts",
        "default": "./src/index.ts"
      }
    },
    "./text-editor.css": "./src/text-editor.css",
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "author": "Karyana Yandi <karyana@yandi.me>",
  "license": "AGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/dafundacom/tokodaim",
    "directory": "packages/text-editor"
  },
  "bugs": {
    "url": "https://github.com/dafundacom/tokodaim/issues"
  },
  "scripts": {
    "format:write": "prettier --write \"**/*.{ts,tsx,mts,js,jsx,mjs,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,mts,js,jsx,mjs,json,md}\"",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@yopem/eslint-config": "catalog:tooling",
    "@yopem/prettier-config": "catalog:tooling",
    "@yopem/typescript-config": "catalog:tooling",
    "eslint": "catalog:tooling",
    "prettier": "catalog:tooling",
